! use left "Cmd" key as Mod4 for XMonad
keycode 133 = Hyper_L Meta_L Super_L
keycode 134 = ISO_Level3_Shift
clear Mod1
clear Mod4
clear Mod5
add Mod1    = Alt_R
add Mod4    = Super_L Super_R Super_L Hyper_L Meta_L
add Mod5    = ISO_Level3_Shift Mode_switch
